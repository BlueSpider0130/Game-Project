{
    "name": "landing",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "build": "ng build",
        "start": "npm run serve",
        "serve": "ng serve --ssl --ssl-cert %APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem --ssl-key %APPDATA%\\ASP.NET\\https\\%npm_package_name%.key --host=localhost --port=4210 --open",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "lint": "ng lint --format table",
        "eslint": "eslint",
        "prettier": "prettier",
        "rimraf": "rimraf",
        "run-s": "run-s",
        "wait-for": "node ../../scripts/wait-for.js",
        "run-ask": "node ../../scripts/run-ask.js",
        "run-arg": "node ../../scripts/run-arg.js",
        "run-watch-seq": "node ../../scripts/run-watch-seq.js",
        "prestart": "node aspnetcore-https",
        "NO:COMMAND:SCRIPTS:SECTION": "----------------------------------SCRIPTS SECTION----------------------------------------------",
        "start:app:landing": "npm run run-watch-seq \"npm run watch:lib:shared\" \"npm run watch:lib:landing\" \"npm run serve:app:landing\"",
        "build:app:landing": "run-s \"build:lib:shared\" \"build:lib:landing\" \"build -- --project=landing\"",
        "serve:app:landing": "npm run serve",
        "build:app:landing:prod": "run-s \"build -- --project=landing --configuration=production\"",
        "watch:app:landing": "run-s \"watch -- --project=landing\"",
        "lint:app:landing": "run-s \"lint landing\"",
        "test:app:landing": "run-s \"test landing\"",
        "storybook:app:landing": "run-s \"ng run landing:storybook\"",
        "storybook-build:app:landing": "run-s \"ng run landing:build-storybook\"",
        "clean:app:landing": "run-s \"rimraf ../../dist/landing\"",
        "NO:COMMAND:DEPENDENCIES:SECTION": "-----------------------------DEPENDENCIES SECTION-----------------------------------------",
        "build:lib:landing": "npm run build:lib:landing --prefix=../itdl/landing",
        "watch:lib:landing": "npm run watch:lib:landing --prefix=../itdl/landing",
        "build:lib:shared": "npm run build:lib:shared --prefix=../itdl/shared",
        "watch:lib:shared": "npm run watch:lib:shared --prefix=../itdl/shared"
    }
}