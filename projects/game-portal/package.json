{
    "name": "game-portal",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --project game-portal",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "serve": "ng serve --ssl --ssl-cert %APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem --ssl-key %APPDATA%\\ASP.NET\\https\\%npm_package_name%.key --host localhost --open",
        "lint": "ng lint --format table",
        "eslint": "eslint",
        "prettier": "prettier",
        "rimraf": "rimraf",
        "run-s": "run-s",
        "wait-for": "node ../../scripts/wait-for.js",
        "run-ask": "node ../../scripts/run-ask.js",
        "run-arg": "node ../../scripts/run-arg.js",
        "run-watch-seq": "node ../../scripts/run-watch-seq.js",
        "watch:server:ssr": "ng run game-portal:server --watch",
        "build:server:ssr": "ng run game-portal:server:development",
        "build:server:ssr:prod": "ng run game-portal:server:production",
        "build:client:ssr": "npm run build:app:game-portal",
        "build:client:ssr:prod": "npm run build:app:game-portal:prod",
        "build:ssr": "run-s \"build:client:ssr\" \"build:server:ssr\"",
        "build:ssr:prod": "run-s \"build:client:ssr:prod\" \"build:server:ssr:prod\"",
        "build:app:game-portal:ssr": "npm run build:ssr",
        "build:app:game-portal:ssr:prod": "npm run build:ssr:prod",
        "build:app:game-portal:ssr:only:prod": "npm run build:server:ssr:prod",
        "dev:ssr": "ng run game-portal:serve-ssr",
        "dev:ssr:express": "ng run game-portal:serve-ssr-express:development",
        "serve:ssr": "node ../../dist/game-portal/server/main.js",
        "prerender": "ng run game-portal:prerender",
        "NO:COMMAND:SCRIPTS:SECTION": "----------------------------------SCRIPTS SECTION----------------------------------------------",
        "build:app:game-portal:prod": "run-s \"build:lib:shared:prod\" \"build:lib:game-portal:prod\" \"build -- --project=game-portal --configuration=production --stats-json\"",
        "build:app:game-portal": " run-s \"build:lib:shared\" \"build:lib:game-portal\" \"build -- --project=game-portal --stats-json\"",
        "start:app:game-portal": "npm run run-watch-seq \"npm run watch:lib:shared\" \"npm run watch:lib:game-portal\" \"npm run serve:app:game-portal\"",
        "serve:app:game-portal": "npm run serve",
        "watch:app:game-portal": "run-s \"watch -- --project=game-portal\"",
        "lint:app:game-portal": "run-s \"lint game-portal\"",
        "test:app:game-portal": "run-s \"test game-portal\"",
        "storybook:app:game-portal": "run-s \"ng run game-portal:storybook\"",
        "storybook-build:app:game-portal": "run-s \"ng run game-portal:build-storybook\"",
        "clean:app:game-portal": "run-s \"rimraf ../../dist/c\"",
        "NO:COMMAND:DEPENDENCIES:SECTION": "-----------------------------DEPENDENCIES SECTION-----------------------------------------",
        "build:lib:game-portal:prod": "npm run build:lib:game-portal:prod --prefix=../itdl/game-portal",
        "build:lib:game-portal": "npm run build:lib:game-portal --prefix=../itdl/game-portal",
        "watch:lib:game-portal": "npm run watch:lib:game-portal --prefix=../itdl/game-portal",
        "build:lib:shared:prod": "npm run build:lib:shared:prod --prefix=../itdl/shared",
        "build:lib:shared": "npm run build:lib:shared --prefix=../itdl/shared",
        "watch:lib:shared": "npm run watch:lib:shared --prefix=../itdl/shared"
    },
    "workspaces": [
        "../../scripts"
    ]
}