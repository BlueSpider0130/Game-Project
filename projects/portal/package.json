{
    "name": "portal",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "npm run start:app:portal",
        "serve": "ng serve --port=4201",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "lint": "ng lint --format table",
        "eslint": "eslint",
        "prettier": "prettier",
        "rimraf": "rimraf",
        "run-s": "run-s",
        "wait-for": "node ../../scripts/wait-for.js",
        "run-ask": "node ../../scripts/run-ask.js",
        "run-arg": "node ../../scripts/run-arg.js",
        "NO:COMMAND:SCRIPTS:SECTION": "----------------------------------SCRIPTS SECTION----------------------------------------------",
        "start:app:portal": "npm-run-all clean:lib:portal --parallel \"start:lib:portal\" \"run-s wait-for:lib:portal serve:app:portal",
        "serve:app:portal": "npm run serve",
        "build:app:portal": "run-s \"build -- --project=portal\"",
        "watch:app:portal": "run-s \"watch -- --project=portal\"",
        "lint:app:portal": "run-s \"lint portal\"",
        "test:app:portal": "run-s \"test portal\"",
        "storybook:app:portal": "run-s \"ng run portal:storybook\"",
        "storybook-build:app:portal": "run-s \"ng run portal:build-storybook\"",
        "clean:app:portal": "run-s \"rimraf ../../dist/portal\"",
        "wait-for:app:portal": "run-s \"wait-for ../../dist/portal\"",
        "NO:COMMAND:DEPENDENCIES:SECTION": "-----------------------------DEPENDENCIES SECTION-----------------------------------------",
        "clean:lib:portal": "npm run clean:lib:portal --prefix=../itdl/portal",
        "wait-for:lib:portal": "npm run wait-for:lib:portal --prefix=../itdl/portal",
        "start:lib:portal": "npm run start:lib:portal --prefix=../itdl/portal"
    }
}
