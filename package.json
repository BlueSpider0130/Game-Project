    "scripts": {
        "ng": "ng",
        "start": "ng serve playground",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "lint": "ng lint --format table",
        "eslint": "eslint",
        "prettier": "prettier",
        "rimraf": "rimraf",
        "run-s": "run-s",
        "wait-for": "node ./scripts/wait-for.js",
        "run-ask": "node ./scripts/run-ask.js",
        "run-arg": "node ./scripts/run-arg.js",
        "NO:COMMAND:GENERATE:SECTION": "----------------------------------GENERATE SECTION----------------------------------------------",
        "generate:app": "run-s \"ng generate application\"",
        "generate:library": "run-s \"ng generate library\"",
        "NO:COMMAND:BUILDAPP:SECTION": "----------------------------------Build App SECTION----------------------------------------------",
        "build:app:landing": "npm run build:app:landing --prefix=./projects/landing",
        "start:app:game-portal:prod": "ng serve --project game-portal --open --configuration=production",
        "start:app:game-portal": "ng serve --project game-portal --open",
        "start:app:game-portal:https": "node ./projects/game-portal/aspnetcore-https && ng serve --ssl --ssl-cert %APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem --ssl-key %APPDATA%\\ASP.NET\\https\\%npm_package_name%.key --host 0.0.0.0 --project game-portal --open --disable-host-check",
        "serve:app:game-portal": "npm run serve:app:game-portal -w=game-portal",
        "build:app:game-portal": "npm run build:app:game-portal --prefix=./projects/game-portal",
        "build:app:game-portal:ssr": "npm run build:app:game-portal:ssr --prefix=./projects/game-portal",
        "build:app:game-portal:ssr:prod": "npm run build:app:game-portal:ssr:prod --prefix=./projects/game-portal"
    },
